<div data-loader="direct" data-component-path="ds2-link-preconfiguration-landing">
    <script type="text/javascript">
        window.preConLanding = {};
        window.preConLanding.currentPath = window.location.pathname;
        window.preConLanding.config = JSON.parse('{\x22activated\x22:true,\x22landingPagesList\x22:[]}');
        window.preConLanding.conBaseUrls = 'https:\/\/configure.bmw\u002Dkz.com\/ru_KZ\/configure\/';
        window.preConLanding.pageType = 'preconfiguration';
        window.preConLanding.isPreConLanding = window.preConLanding.config.landingPagesList.some(function (item) {
            let preConUrl;
            try {
                preConUrl = new URL(item.target).pathname;
            } catch (e) {
                preConUrl = item.target;
            }
            return window.preConLanding.currentPath === preConUrl;
        });

        if (window.preConLanding.isPreConLanding && window.digitals2 && window.digitals2.tracking && window.digitals2.tracking.pageHelper && window.trackingConfiguration && window.trackingConfiguration.digitalData && window.trackingConfiguration.digitalData.pages[0]) {
            window.digitals2.tracking.pageHelper.setPageType(window.trackingConfiguration.digitalData.pages[0], window.preConLanding.pageType);
        }
    </script>
</div>


