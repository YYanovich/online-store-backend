<script>
    const smartbanner = document.querySelector(".smartbanner");
    const metaApple = document.querySelector('meta[name="apple-itunes-app-custom"]');
    const isTouch = {
        android: (/Android/i).test(navigator.userAgent),
        iPhone: (/iPhone|iPod/i).test(navigator.userAgent),
        iPad: (/iPad/i).test(navigator.userAgent) || (navigator.platform === 'MacIntel' && navigator.maxTouchPoints > 1),
    };
    isTouch.iOS = isTouch.iPhone || isTouch.iPad;
    const browser = {
        iOSSafari: isTouch.iOS && !(/(CriOS|FxiOS|FBIOS|OPiOS|mercury)/i.test(navigator.userAgent)),
    };

    if (browser.iOSSafari && metaApple) {
        metaApple.name = 'apple-itunes-app';
    }
    else if (smartbanner !== null && (isTouch.iOS || isTouch.android)) {
        const toggleSmartbannerVisibility = state => {
            smartbanner.style.display = state;
        };

        if (!sessionStorage.getItem("smartbanner-closed")) {
            toggleSmartbannerVisibility("block");
            require(['ds2-smartbanner'], function (Ds2SmartBanner) {
                const element = smartbanner.querySelector(".ds2-smartbanner");

                requestAnimationFrame(function () {
                    const smartBanner = new Ds2SmartBanner({element, toggleSmartbannerVisibility});
                })
            });
        }
    }
</script>





    


