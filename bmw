<div class="ds2-component ds2-tracking-aware ds2-micro-story" data-loader="amdLoader" data-component-path="ds2-microstory" data-tracking-component="{&#34;attributes&#34;:{&#34;variant&#34;:&#34;4 columns&#34;},&#34;category&#34;:{&#34;primaryCategory&#34;:&#34;Detail&#34;,&#34;componentType&#34;:&#34;integrated&#34;},&#34;componentInfo&#34;:{&#34;componentID&#34;:&#34;/content/bmw/marketB4R1/bmw_kz/ru_KZ/all-models/m-series/jcr:content/par/columncontrol_69dc_c_1537035905/par-4col/microstory_925e : 1627040013688&#34;,&#34;componentHeadline&#34;:&#34;BMW M8 Competition (F91, F92, F93)&#34;,&#34;componentName&#34;:&#34;micro_story&#34;,&#34;componentType&#34;:&#34;integrated&#34;}}">
    
    





    
    
        <div class="ds2-micro-story" data-loader="amdLoader" data-component-path="ds2-micro-story-item">
    
    
    <div class="ds2-expand--float float-right ">
        

        
        
    
    
    <h3 class="ds2-micro-story--title  ">
        
        
            BMW M8 COMPETITION (F91, F92, F93)
        
    </h3>


        <div class="ds2-micro-story--media-container small-12 medium-6 large-6">
            <div>
  
  
  
  
  
  
  
  
  
  
  
  
  

  
    
    
    
    
    
    

    
    

    <picture data-desktop-aspect-ratio="1.78">
      
      
      
      
      
      
  
