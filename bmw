<script type="text/javascript">
    function removeAnchorWall() {
        var anchorWall = document.querySelector('.ds2-anchor-wall');
        if (anchorWall) {
            anchorWall.parentElement.removeChild(anchorWall);
            anchorWall = null;
        }
    }
    // remove anchor wall right away if there is no hash or if the hash is a different function
    if (window.location.hash === '' || window.location.hash.startsWith("#/")) {
        removeAnchorWall();
    }
</script>

